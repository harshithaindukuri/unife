<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.indukuri.unife">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/unife"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AccountActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".WalletDetails"
            android:label="@string/app_name" />
        <activity
            android:name=".CreditDetails"
            android:label="@string/app_name" />
        <activity
            android:name=".DebitDetails"
            android:label="@string/app_name" />
        <activity
            android:name=".NetBankDetails"
            android:label="@string/app_name" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".DriverDetails" />
        <activity android:name=".PassengerEntrance" />
        <activity android:name=".Pass_Driv_entrnc" />
        <activity android:name=".GenerateTicket" />
        <activity android:name=".StartRide" />
        <activity android:name=".Avail_modes" />
        <activity android:name=".Track_confirm" />
        <activity android:name=".DriverRequests" />
        <activity android:name=".CompleteActivity"></activity>
    </application>

</manifest>